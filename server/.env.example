# Copy this file to .env and update the values with your credentials.
# PORT is optional; defaults to 4000 when omitted.
PORT=4000

# Address where contact + booking notifications should arrive.
CONTACT_TO_EMAIL=info@example.com

# SMTP configuration used by nodemailer.
SMTP_HOST=smtp.your-provider.com
SMTP_PORT=587
SMTP_USER=apikey-or-username
SMTP_PASS=super-secret-password
